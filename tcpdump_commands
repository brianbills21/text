yum install -y tcpdump  
tcpdump 192.168.1.9  
tcpdump -i ens33  
tcpdump -c 5 -i ens33   
tcpdump -A -i ens33  
tcpdump -D  
tcpdump -XX -i ens33 | tail -f  
tcpdump -XX -i ens33 | more  
tcpdump -w 0001.pcap -i ens33  
tcpdump -r 0001.pcap   
tcpdump -n -i ens33  
tcpdump -i ens33 tcp  
tcpdump -i ens33 port 22  
tcpdump -i ens33 src 192.168.1.11  
tcpdump -i ens33 src 192.168.134.142  
tcpdump -i ens33 dst 192.168.134.142  
